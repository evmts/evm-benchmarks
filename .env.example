# EVM Benchmark Suite Configuration
# Copy this file to .env and adjust values as needed

# Python environment
PYTHONPATH=.

# Benchmark configuration
BENCHMARK_ITERATIONS=10
BENCHMARK_WARMUP=3
BENCHMARK_TIMEOUT=300

# EVM paths (optional, will auto-detect if not set)
GETH_PATH=
RETH_PATH=
EVMS_PATH=

# Benchmark output
RESULTS_DIR=./results
EXPORT_FORMAT=json

# Docker configuration
DOCKER_IMAGE=evm-bench:latest
DOCKER_NETWORK=bridge

# Logging
LOG_LEVEL=INFO
LOG_FILE=benchmark.log

# Performance tuning
CPU_AFFINITY=
NICE_LEVEL=0

# Network configuration (for remote benchmarks)
RPC_ENDPOINT=http://localhost:8545
CHAIN_ID=1337

# Test configuration
TEST_COVERAGE=true
TEST_PARALLEL=true

# CI/CD
CI=false
GITHUB_TOKEN=